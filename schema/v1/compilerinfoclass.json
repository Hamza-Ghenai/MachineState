{
   "$schema": "http://json-schema.org/draft-07/schema#",
   "title": "CompilerInfoClass",
   "$id": "#compilerinfoclass",
   "description": "General class for all parent classes for executables",
   "type": "object",
   "properties": {
      "Version": {
         "description": "Version of the executable",
         "type": "string"
      },
      "Path": {
         "description": "Absolute path of the executable",
         "type": "string"
      }
   },
   "required": ["Version", "Path"]
}